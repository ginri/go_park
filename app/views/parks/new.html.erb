<h1>Parks#new</h1>
<p>Find me in app/views/parks/new.html.erb</p>
<h1>新規投稿</h1>
<%= form_with model: Park.new do |f| %>
  <h4>公園名</h4>
  <%= f.text_field :name %>

  <%= f.collection_select(:prefecture_id, Prefecture.all, :id, :name) %>

  <h4>説明文</h4>
  <%= f.text_area :description %>

 <h4>address</h4>
<%= f.text_area :address %>


  <%= f.submit '投稿' %>
<% end %>