<h3><%= @prefecture.name %>の公園</h3>
<div class="col-12">
  <div class="card-columns">
    <% @parks.each do |park| %>
      <div class="card" style="width: 18rem;">
        <svg class="bd-placeholder-img card-img-top" width="30%" height="180" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: Image cap"><title>Placeholder</title><rect fill="#868e96" width="100%" height="100%"/><text fill="#dee2e6">Image cap</text></svg>
        <div class="card-body">
          <h5 class="card-title"><%= link_to park.name, park%></h5>
        </div>
      </div>
    <% end %>
  </div>
</div>

